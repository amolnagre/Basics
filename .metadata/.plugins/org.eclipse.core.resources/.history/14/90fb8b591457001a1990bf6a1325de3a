package Assignment1;

import java.util.Scanner;

public class ProgramFirst {
	/*1. A number is given as input.
	 * Find the odd digits in the number, 
	 * add them and find if the sum is odd or not.if even return -1, if odd return 1
		input:52315
		logic:5+3+1+5=14(even)
		output:-1
		input:1112
		logic:1+1+1=3(odd)
		output:1

	 * 
	 */
	public static void main(String[] args) {
		
		int num;
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Number: ");
		num=sc.nextInt();
		
		for(int i=0;i<num;i++)
		{
			num=num+1;
			System.out.println(num);
		}
		
		
		
		
		
		
		
	}

}
